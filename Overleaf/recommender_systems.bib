}

@article{khojamli_survey_2021,
	title = {Survey of similarity functions on neighborhood-based collaborative filtering},
	volume = {185},
	issn = {09574174},
	url = {https://linkinghub.elsevier.com/retrieve/pii/S0957417421008939},
