	file = {Zhong et al. - 2024 - A Comprehensive Survey on Automatic Knowledge Grap.pdf:/Users/eva/Zotero/storage/N9EKMXWI/Zhong et al. - 2024 - A Comprehensive Survey on Automatic Knowledge Grap.pdf:application/pdf},
}

